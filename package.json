{
  "name": "jayudam-backend",
  "version": "0.1.0",
  "description": "alpacasalon-backend",
  "homepage": "https://github.com/rmfpdlxmtidl/jayudam-backend#readme",
  "bugs": {
    "url": "https://github.com/rmfpdlxmtidl/jayudam-backend/issues",
    "email": "gwak2837@kakao.com"
  },
  "license": "GPL-3.0-only",
  "author": "Taeuk Gwak <gwak2837@kakao.com>",
  "main": "dist/index.js",
  "repository": "https://github.com/rmfpdlxmtidl/jayudam-backend.git",
  "scripts": {
    "clean": "rm -rf .yarn/cache .yarn/unplugged .yarn/install-state.gz out .pnp.*",
    "dev": "NODE_ENV=development node esbuild.js & nodemon -r dotenv/config out/index.cjs dotenv_config_path=.env.development.local",
    "build": "NODE_ENV=production node esbuild.js",
    "start": "NODE_ENV=production node -r dotenv/config out/index.cjs dotenv_config_path=.env.local",
    "test": "NODE_ENV=test jest",
    "prettier": "prettier . --write",
    "eslint": "eslint . --fix --ignore-path .gitignore",
    "type": "tsc --pretty --noEmit",
    "pre-push": "yarn type",
    "generate": "rm -rf src/graphql/generated && graphql-codegen --config codegen.yml",
    "generate-db": "database/generate.sh",
    "export": "database/export.sh",
    "import": "database/import.sh",
    "postinstall": "husky install"
  },
  "dependencies": {
    "apollo-server-core": "^3.8.1",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.8.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.38",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.6",
    "esbuild": "^0.14.42",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=18.2.0"
  },
  "private": true,
  "packageManager": "yarn@3.2.1",
  "type": "module"
}
