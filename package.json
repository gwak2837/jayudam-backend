{
  "name": "jayudam-backend",
  "version": "0.1.0",
  "description": "jayudam-backend",
  "homepage": "https://github.com/rmfpdlxmtidl/jayudam-backend#readme",
  "bugs": {
    "url": "https://github.com/rmfpdlxmtidl/jayudam-backend/issues",
    "email": "gwak2837@kakao.com"
  },
  "license": "GPL-3.0-only",
  "author": "Taeuk Gwak <gwak2837@kakao.com>",
  "main": "dist/index.js",
  "repository": "https://github.com/rmfpdlxmtidl/jayudam-backend.git",
  "scripts": {
    "dev": "src/dev.sh",
    "build": "NODE_ENV=production node esbuild.js",
    "start": "NODE_ENV=production node -r dotenv/config out/index.cjs dotenv_config_path=.env.local",
    "test": "NODE_ENV=test jest",
    "clean": "rm -rf .yarn/cache .yarn/unplugged .yarn/install-state.gz out .pnp.*",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "format": "prettier . --write",
    "type": "tsc --pretty --noEmit",
    "pre-push": "yarn type",
    "generate": "graphql-codegen --config codegen.yml --errors-only",
    "pgtyped": "database/pgtyped.sh",
    "export": "tsc --project database/tsconfig.json && node database/dist/export.js",
    "import": "tsc --project database/tsconfig.json && node database/dist/import.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.2.3",
    "@graphql-tools/merge": "^8.3.1",
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-core": "^3.10.0",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.10.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "ioredis": "^5.2.2",
    "jsonwebtoken": "^8.5.1",
    "lunar-javascript": "^1.2.32",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.2.9",
    "pg": "^8.7.3",
    "telegraf": "^4.8.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.0",
    "@graphql-codegen/schema-ast": "^2.5.0",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-resolvers": "2.7.2",
    "@pgtyped/cli": "^1.0.2",
    "@pgtyped/query": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.2",
    "@types/pg": "^8.6.5",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/sharp": "^0.30.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.14",
    "esbuild": "^0.14.50",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "pg-copy-streams": "^6.0.2",
    "prettier": "^2.7.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18.2.0"
  },
  "private": true,
  "packageManager": "yarn@3.2.1",
  "type": "module"
}
